//USer function Template for C++
class Solution{
  public :
       long long int minOperations(int N) {
        //code here
        int i = 0, j = N - 1;
        x = N/2;
        if(N%2!=0)
        ans = (x(x+1));
        else
        ans=x*x;
        return ans;
  }
};
//{Driver Code Statrs
int main() {
    int t;
    cin >> t;
    while(t--) {
    int n;
    cin >> n;
    Solution ob;
    cout << ob.minOperations(n) << endl;
    }
    return 0;
  }
//} Driver Code Ends
